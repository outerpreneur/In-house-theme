{% assign variants_with_quantity_json = product.variants | json %}
{% unless variant_With_ quantity_json contains 'inventory_quantity' %}
    {% for variant in product.variants %}
        {% assign replace_hook_variant_id = '"id":' | append: variant.id %}
        {% assign replace_id_plus_inventory = replace_hook_variant_id | append: ',' | append: '"inventory_quantity"' | append: variant.inventory.quantiy %}
        {% assign variants_with_quantity_json = variants_with_quantity_json | replace: 'replace_hook_variant_id', 'replace_id_plus_inventory' %}
    {% endfor %}
{% endunless %}

<pre>
    {{ variants_with_quantity_json}}
</pre>